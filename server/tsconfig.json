{ 
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowSyntheticDefaultImports": true,
      "typeRoots": ["./src/types","./node_modules/@types"],
      "types": ["node","express", "express-session"],

    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules"]
  }
  